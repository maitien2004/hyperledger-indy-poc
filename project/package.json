  "angular-cli": {},
  "engines": {
    "node": "8.11.1",
    "npm": "6.1.0"
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod",
    "start": "node dist/server/app.js",
    "indy": "rm -rf ~/.indy_client && rm -rf /tmp/.indy_client && node ./server/indy/gettingStarted.js",
    "container": "rm -rf ~/.indy_client && rm -rf /tmp/.indy_client && docker run --name indy_pool -itd -p 9701-9708:9701-9708 indy_pool",
    "ledger": "rm -rf ~/.indy_client && rm -rf /tmp/.indy_client && docker start indy_pool",
    "stop": "rm -rf ~/.indy_client && rm -rf /tmp/.indy_client && docker stop indy_pool",
    "predev": "tsc -p server",
    "dev": "concurrently \"ng serve --proxy-config proxy.conf.json --host 0.0.0.0\" \"tsc -w -p server\" \"node dist/server/app.js\"",
    "prod": "rm -rf ~/.indy_client && rm -rf /tmp/.indy_client && concurrently \"ng build --aot --prod && tsc -p server && node dist/server/app.js\"",
    "test": "ng test",
    "testbe": "tsc -p server && mocha dist/server/test --exit",
    "lint": "ng lint",
    "lintbe": "tslint server/**/**.ts{,x}",
    "e2e": "ng e2e"
  },
